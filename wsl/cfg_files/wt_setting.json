		},
		{
			"command": {
				"action": "moveTab",
				"direction": "forward"
			},
			"name": "MyCMD: moveTab right",
			"keys": "shift+right"
		},
		{
			"command": "togglePaneZoom",
			"name": "MyCMD: show all window",
			"keys": "shift+alt+a"
		},
		{
			"command": {
				"action": "multipleActions",
				"actions": [
					// create newPane
					{"action": "newTab", "index": 1},
					// Go back to the prevTab and close
					{ "action": "prevTab", "tabSwitcherMode": "inOrder" },
					//close old tab
					{"action": "closePane"}
					//go to new tab
					//{ "action": "nextTab", "tabSwitcherMode": "inOrder" }
				]
			},
			"name": "MyCMD: multiple cmd Actions",
			"keys": "ctrl+d"
		},
