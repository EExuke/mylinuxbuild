        {
            "command": 
            {
                "action": "sendInput",
                "input": "tftp -gr mainApp 10.0.0.199\r\n"
            },
            "keys": "ctrl+0",
            "name": "MyCMD: send cmd 0"
        },
		{
			"command": {
				"action": "moveTab",
